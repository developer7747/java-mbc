.latest_offer p{padding:10px;}
.offer_arrow { float:right; padding:0px; color:#F00; font-size:20px;}
.offer_arrow a{text-decoration:none;}

.offers_box{ float:left; width:188px; height:300px; padding:0;  margin:10px 0 5px 15px; background:#e4e4e4; box-shadow:2px 2px 2px #ccc;}
.offers_box img{border-top:7px solid #ec1b2e; border-bottom:3px solid #fff;}
.offers_box p{padding:5px 10px;}
.offers_box h3 {text-align:center; font-size:16px; color:#ec1b2e; padding-top:15px;}
    .offers_box h3 a {text-decoration:none; color:#ec1b2e; }


