/*Master Page Tool Tips*/
a.tooltips {  position: relative;  display: inline;}
a.tooltips font { padding:7px;   position: absolute;  min-width:140px;  color: #FFFFFF;  background: #6fb129;    /*line-height: 20px;*/  text-align: center;  visibility: hidden;  border-radius: 6px; font-weight:bold;}
a.tooltips font:after {  content: '';  position: absolute;  right: 100%;	top: 20%;  margin-left: -8px;  width: 0; height: 0;  border-top: 8px solid transparent;  border-right: 8px solid #6fb129;  border-left: 8px solid transparent;  border-bottom: 8px solid transparent;}
a:hover.tooltips font { font-size:11px;  visibility: visible;  opacity: 1;  top: 0%;  left: 230%;  margin-left: -76px;  z-index: 999;}

.grow {
    height: 50px; /* Origional height */
    width: 100%; /* Origional width */
    margin: 5px 1% 5px 1%; /* Just for presentation (Not required) */
    float: left; /* Just for presentation (Not required) */
    position: relative; /* Just for presentation (Not required) */
    transition:height 0.5s; /* Animation time */
    -webkit-transition:height 0.5s; /* For Safari */
}

.grow:hover {
    height: 300px; /* This is the height on hover */
    width:100%;
}

.grow img:after { display:none;
}

.grow:hover img {
    display: inline;
}

.bigGrow {
   height: 300px; /* Origional height */
    width: 100%; /* Origional width */
    margin: 5px 1% 5px 1%; /* Just for presentation (Not required) */
    float: left; /* Just for presentation (Not required) */
    position: relative; /* Just for presentation (Not required) */
    transition:height 0.5s; /* Animation time */
    -webkit-transition:height 0.5s; /* For Safari */
}

.CloseGrow{
    display : none;
}

